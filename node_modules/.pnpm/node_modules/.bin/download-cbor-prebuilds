#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/bin/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/bin/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules/cbor-extract/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/cbor-extract@2.2.0/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../cbor-extract/bin/download-prebuilds.js" "$@"
else
  exec node  "$basedir/../cbor-extract/bin/download-prebuilds.js" "$@"
fi
