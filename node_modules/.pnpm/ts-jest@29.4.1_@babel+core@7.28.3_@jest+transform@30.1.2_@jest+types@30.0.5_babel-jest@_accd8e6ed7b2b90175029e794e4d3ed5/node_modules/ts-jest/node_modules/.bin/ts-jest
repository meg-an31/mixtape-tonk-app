#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_accd8e6ed7b2b90175029e794e4d3ed5/node_modules/ts-jest/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_accd8e6ed7b2b90175029e794e4d3ed5/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_accd8e6ed7b2b90175029e794e4d3ed5/node_modules/ts-jest/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/ts-jest@29.4.1_@babel+core@7.28.3_@jest+transform@30.1.2_@jest+types@30.0.5_babel-jest@_accd8e6ed7b2b90175029e794e4d3ed5/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
