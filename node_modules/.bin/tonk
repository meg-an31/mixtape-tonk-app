#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/cli/bin/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/cli/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/cli/bin/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/cli/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules/@tonk/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/@tonk+cli@0.2.28_@swc+core@1.13.5_@swc+helpers@0.5.17__@swc+wasm@1.13.5_@types+node@24._2425a7e7530900030f4f4ecf935c6730/node_modules:/Users/meganwalker/all-apps/app-one/basic-mixtape/basic-mixtape/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@tonk/cli/bin/tonk.js" "$@"
else
  exec node  "$basedir/../@tonk/cli/bin/tonk.js" "$@"
fi
